def main():
    stroka = input()

    max_podstroka = ''
    max_len = 0
    podstroka = ''

    for el in stroka:
        if el not in podstroka:
            podstroka += el
        else:
            if len(podstroka) > max_len:
                max_podstroka, max_len = podstroka,  len(podstroka)
                podstroka = ''

                
    print(max_podstroka)

if __name__ == '__main__':
    main()


